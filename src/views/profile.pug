	body
		#nav
			if user
				#myTopnav(class='topnav')
					a(href='index') Home
					a(href='#') Profile
					a(href='/newpost') New Post
					a(href='allposts') All Posts
					a(href='logout') Logout
					a(href='javascript:void(0);' style='font-size:15px;' class='icon' onclick='myFunction()') &#9776 
			
			else
				#myTopnav(class='topnav')
					a(href='index') Home
					a(href='#') Profile
					a(href='/newpost') New Post
					a(href='register') Register
					a(href='javascript:void(0);' style='font-size:15px;' class='icon' onclick='myFunction()') &#9776 

		#profile
			h2 Hi #{user.name}, this is your profile.
			h3 your profile name:
			#profile-list
				p #{user.name}
			h3 your email:
			#profile-list 
				p #{user.email}
			h3 your password:
			#profile-list 
				p #{user.password}


